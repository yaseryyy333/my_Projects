    @{
        var user = ViewBag.user as myUser;
        if (ViewBag.isAuthenticated)
        {

            <form asp-action="chat" method="post">
                <input type="hidden" name="id" asp-for="@user.Id" />
                <a onclick="putValue(this)">
                    <div class="my-account chat">
                        <div class="user">
                            <span class="chat-number"></span>
                            <i class="fa fa-weixin"></i>
                        </div>
                    </div>
                </a>
            </form>

            <!-- Start  If User Sign In -->

            <div onclick="showUserEdit()" id="edit" class="my-account user">
                <a>
                    <div class="user">
                        <i class="fa fa-edit"></i>
                    </div>
                </a>
            </div>

            <div onclick="Logout()" id="out" class="my-account user">
                <a>
                    <div class="user">
                        <i class="fa fa-door-open"></i>
                    </div>
                </a>
            </div>

            <a>
                <div onclick="showUserAccount()" class="my-account user">
                    <div id="user_arrow" class="user">
                        <span></span>
                        <i class="fa fa-user"></i>
                        <i class="fa fa-chevron-down"></i>
                    </div>
                </div>
            </a>

        }
        else
        {
            <!-- End If user Sign In -->
            <!-- Start If User  -->
            <a asp-action="login_register">
                <div onclick="showUserAccount()" class="my-account user">
                    <div id="user_arrow" class="user">
                        <span></span>
                        <i class="fa fa-user"></i>
                    </div>

                </div>
            </a>
        }
    }

