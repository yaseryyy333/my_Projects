    <script>

        function AlertFunction(element) {
            element.style.opacity = '0';
            element.style.zIndex = '-1';
        }


        function EditUserInformation() {

            //Start variables Forms
            var fullName = document.getElementById("user_UserName");
            var phoneNumber = document.getElementById("user_PhoneNumber");
            var address = document.getElementById("user_Address");
            var password = document.getElementById("user_PasswordHash");
            var branchId = document.getElementById("user_BranchId");


            var fullNameError = document.getElementById("user_UserNameError");
            var phoneNumberError = document.getElementById("user_PhoneNumberError");
            var addressError = document.getElementById("user_AddressError");
            var passwordError = document.getElementById("user_PasswordHashError");
            var branchIdError = document.getElementById("user_BranchIdError");


            var AlertItem = document.getElementById('AlertId');
            //End Variables Forms


            //Start validation
            if (fullName.value == "") {
                fullNameError.innerHTML = "يجب ادخال اسم";
            }
            else {
                fullNameError.innerHTML = "";
            }
            if (phoneNumber.value == "") {
                phoneNumberError.innerHTML = "يجب ادخال رقم الجوال";
            }
            else {
                phoneNumberError.innerHTML = "";
            }
            if (address.value == "") {
                addressError.innerHTML = "يجب ادخال العنوان";
            }
            else {
                addressError.innerHTML = "";
            }
            if (password.value == "") {
                passwordError.innerHTML = "يجب ادخال كلمة مرور جديدة";
            }
            else {
                passwordError.innerHTML = "";
            }
            if (branchId.value == "") {
                branchIdError.innerHTML = "يجب اختيار الفرع";
            }
            else {
                branchIdError.innerHTML = "";
            }
            //End validation

            //Start Variables Of Ajax
            var data = {
                Name: fullName.value,
                PhoneNumber: phoneNumber.value,
                Address: address.value,
                Password: password.value,
                BranchID: parseInt(branchId.value)
            };
            var dataSend = JSON.stringify(data);
            var url = "/api/user/UpdateUser?id=" + localStorage.getItem("id");
            //End Variables Of Ajax


            //Start Ajax
            $.ajax({
                type: "PUT",
                url: url,
                data: dataSend,
                headers: { "Authorization": "Bearer " + localStorage.getItem("Authorization") },
                contentType: "application/json",
                dataType: "json",
                success: function (DataReceive) {
                    if (DataReceive.succeeded) {
                        AlertItem.innerHTML = "تم الحفظ بنجاح";
                        AlertItem.style.backgroundColor = "green";
                        AlertItem.style.opacity = '1';
                        AlertItem.style.zIndex = '100';
                        setTimeout(function () { AlertFunction(AlertItem); }, 3000);
                    }
                    else {
                        AlertItem.innerHTML = "يجب أن تتكون كلمة المرور من 5 أحرف على الأقل";
                        AlertItem.style.backgroundColor = "red";
                        AlertItem.style.opacity = '1';
                        AlertItem.style.zIndex = '100';
                        setTimeout(function () { AlertFunction(AlertItem); }, 3000);
                    }

                    console.log(DataReceive);
                },
                error: function (error) {
                    var er = JSON.parse(JSON.stringify(error));
                    if (password.value.length < 5) {
                        AlertItem.innerHTML = er.responseJSON.errors.Password[0];
                    }
                    if (
                        password.value == "" ||
                        branchId.value == "" ||
                        address.value == "" ||
                        phoneNumber.value == "" ||
                        fullName.value == ""
                    ) {
                        AlertItem.innerHTML = "يجب تعبئة جميع الحقول";
                    }
                    AlertItem.style.backgroundColor = "red";
                    AlertItem.style.opacity = '1';
                    AlertItem.style.zIndex = '100';
                    setTimeout(function () { AlertFunction(AlertItem); }, 3000);


                    //console.log(error.responseJSON.errors.PhoneNumber[0]);
                }
            })
            //End Ajax

        }

        function AlertFunction(element) {
            element.style.opacity = '0';
            element.style.zIndex = '-1';
        }
        //End Function To SignIn==============



        //Start Function To SignOut

        function Logout() {
            var url = "/api/user/SignOut";
            //End Variables Of Ajax
            //Start Ajax
            $.ajax({
                type: "GET",
                url: url,
                headers: { "Authorization": "Bearer " + localStorage.getItem("Authorization") },
                contentType: "application/json",
                dataType: "json",
                success: function (success) {
                    localStorage.removeItem("id");
                    localStorage.removeItem("Authorization");
                    localStorage.removeItem("isAuthenticated");
                    localStorage.removeItem("isAuthenticated");
                    location.href = "/";
                }
            });

        }


        function putValue(element) {
            //element.parentNode.children[0].value = localStorage.getItem("id");
            element.parentNode.submit();
        }

        //End Function To SignOut






    </script>

